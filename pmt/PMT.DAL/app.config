      <clear/>
      <add name="SqlServer" type="SubSonic.SqlDataProvider, SubSonic" connectionStringName="PMT.DAL.Properties.Settings.PmtConnectionString" generatedNamespace="PMT.DAL"/>
    </providers>
  </SubSonicService>
</configuration>